{
  "expo": {
    "name": "app",
    "slug": "esd-mobile-app-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "esd-mobile-app-project",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pin_toilettes.png",
      "resizeMode": "contain",
      "backgroundColor": "#7478B7"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esdprojectappmobile.esdmobileappproject"
    },
    "plugins": ["expo-router", "expo-localization", "expo-font"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.esdprojectappmobile.esdmobileappproject"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e7cdc307-2148-4e8a-8389-6e63c401ed7e"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "esd-project-app-mobile"
  }
}
